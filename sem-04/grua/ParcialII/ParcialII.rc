        MENUITEM SEPARATOR
        MENUITEM "&Salir",                      ID_APP_EXIT
    END
    POPUP "&Edición"
    BEGIN
        MENUITEM "&Deshacer\tCtrl+Z",           ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cor&tar\tCtrl+X",             ID_EDIT_CUT
        MENUITEM "&Copiar\tCtrl+C",             ID_EDIT_COPY
        MENUITEM "&Pegar\tCtrl+V",              ID_EDIT_PASTE
    END
    POPUP "&Ver"
    BEGIN
        MENUITEM "&Barra de herramientas",      ID_VIEW_TOOLBAR
        MENUITEM "&Barra de estado",            ID_VIEW_STATUS_BAR
    END
    POPUP "Ay&uda"
    BEGIN
        MENUITEM "&Acerca de ParcialII...",     ID_APP_ABOUT
    END
    POPUP "Parcial"
    BEGIN
        POPUP "Grua"
        BEGIN
            MENUITEM "Dibujar",                     ID_GRUA_DIBUJAR
            POPUP "Rotar soporte"
            BEGIN
                MENUITEM "Derecha",                     ID_ROTARSOPORTE_DERECHA
                MENUITEM "Izquierda",                   ID_ROTARSOPORTE_IZQUIERDA
            END
            POPUP "Rotar canasta"
            BEGIN
                MENUITEM "Derecha",                     ID_ROTARCANASTA_DERECHA
                MENUITEM "Izquierda",                   ID_ROTARCANASTA_IZQUIERDA
            END
        END
        POPUP "Elipse"
        BEGIN
            MENUITEM "Dibujar",                     ID_ELIPSE_DIBUJAR
            MENUITEM "Cambiar color",               ID_ELIPSE_CAMBIARCOLOR
        END
    END
